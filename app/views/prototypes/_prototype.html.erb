<div class="card">
<%= link_to image_tag(prototype.image), root_path, class: :card__img %>
  <div class="card__body">
    <%= link_to prototype.title, prototype_path(prototype.id), class: :card__title %>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{prototype.user_name}", user_path(prototype.user_id), class: :card__user %> 
  </div>
</div>